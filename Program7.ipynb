        "\n",
        "Y = np.array([[5,6], [7,8]])\n",
        "\n",
        "print(\"Matrix X is :\\n \",X)\n",
        "\n",
        "print(\"Matrix Y is : \\n\",Y)\n",
        "\n",
        "a=np.power(X,2)\n",
        "\n",
        "print(\"X^2=\",a)\n",
        "\n",
        "result = a + 2 * Y\n",
        "\n",
        "print(\"X^2+2*Y is \\n \",result)"
      ],
